{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Launch (FrontitudeToResxXlsx)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build-and-prepare-output",
      "program": "${workspaceFolder}/FrontitudeToResxXlsx/bin/Debug/net8.0/FrontitudeToResxXlsx.dll",
      "args": [
        "TestData/Frontitude.json",
        "Output/output.xlsx"
      ],
      "cwd": "${workspaceFolder}/FrontitudeToResxXlsx",
      "console": "integratedTerminal",
      "stopAtEntry": false
    }
  ]
}
